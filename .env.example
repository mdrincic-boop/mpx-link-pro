# Supabase Configuration (Optional - for cloud features)
# Copy this file to .env and fill in your credentials

SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here

# Application Settings
APP_VERSION=2.0
APP_NAME=MPX over IP Pro

# Default Network Settings (can be overridden in GUI)
DEFAULT_PORT=9000
DEFAULT_SAMPLE_RATE=192000
DEFAULT_BLOCK_SIZE=1024

# Logging Settings
ENABLE_SESSION_LOGGING=true
LOG_DIRECTORY=logs
ENABLE_AUDIO_RECORDING=false

# Performance Settings
ENABLE_STATISTICS=true
STATISTICS_INTERVAL=1000
